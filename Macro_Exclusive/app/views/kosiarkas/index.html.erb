<p id="notice">
	<%= notice %>
</p>

<h1>Kosiarkas</h1>

<table>
	<thead>
		<tr>
			<th colspan="3"></th>
		</tr>
	</thead>

	<tbody>
		<% @kosiarkas.each do |kosiarka| %>
		<tr>
			<td>
				<%= link_to 'Show', kosiarka %>
			</td>
			<td>
				<%= link_to 'Edit', edit_kosiarka_path(kosiarka) %>
			</td>
			<td>
				<%= link_to 'Destroy', kosiarka, method: :delete, data: { confirm: 'Are you sure?' } %>
			</td>
		</tr>
		<% end %>
	</tbody>
</table>

<br>

	<%= link_to 'New Kosiarka', new_kosiarka_path %>
